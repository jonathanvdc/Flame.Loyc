using System;
using LeMP;

namespace Test
{
    public struct Vector
    {
        public this(public double X, public double Y) { }

        public double LengthSquared
        {
            get { return X * X + Y * Y; }
            set { Length = Math.Sqrt(value); }
        }

        public double Length
        {
            get { return Math.Sqrt(LengthSquared); }
            set
            {
                double ratio = value / Length;
                X *= ratio;
                Y *= ratio;
            }
        }
    }

