public class X
{
    public int abs(int n)
    {
        if (n >= 0)
            return n;
        else
            return -n;
    }

    public X getThis() { return this; }

    public int factorial(int n)
    {
        int result = 1;
        int i = 1;
        while (true)
        {
            if (i > n) break;
            result *= i;
            i++;
        }
        return result;
    }
}
